{
  "name": "drinks",
  "version": "0.0.0-semantically-released",
  "description": "Static site and PWA to showcase your favorite cocktails",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/drinks.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/drinks/issues"
  },
  "scripts": {
    "build": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "preserve": "yarn build",
    "serve": "gatsby serve",
    "test": "yarn lint",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@fontsource/source-sans-pro": "4.1.0",
    "@reach/dialog": "0.12.1",
    "@reach/skip-nav": "0.12.1",
    "@reach/visually-hidden": "0.12.0",
    "@wkovacs64/normalize.css": "8.0.1",
    "@xstate/react": "1.2.2",
    "algoliasearch": "4.8.3",
    "dotenv": "8.2.0",
    "gatsby": "2.30.1",
    "gatsby-image": "2.9.0",
    "gatsby-plugin-algolia": "0.16.2",
    "gatsby-plugin-emotion": "5.2.0",
    "gatsby-plugin-manifest": "2.10.0",
    "gatsby-plugin-netlify": "2.9.0",
    "gatsby-plugin-offline": "3.8.0",
    "gatsby-plugin-react-helmet": "3.8.0",
    "gatsby-plugin-sharp": "2.12.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-contentful": "4.4.0",
    "gatsby-source-filesystem": "2.9.0",
    "gatsby-transformer-remark": "2.14.0",
    "gatsby-transformer-sharp": "2.10.0",
    "git-current-commit": "1.1.0",
    "lodash": "4.17.20",
    "prop-types": "15.7.2",
    "query-string": "6.13.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-icons": "4.1.0",
    "xstate": "4.15.3"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@wkovacs64/prettier-config": "3.0.0",
    "commitizen": "4.2.2",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.17.0",
    "eslint-plugin-wkovacs64": "11.0.1",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "semantic-release": "17.3.1"
  }
}
