    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.24.0",
    "eslint-plugin-wkovacs64": "13.0.1",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "semantic-release": "17.4.2"
  }
