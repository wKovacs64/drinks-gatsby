  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "cross-env NODE_ENV=test eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "preserve": "yarn build",
    "serve": "gatsby serve",
    "test": "yarn lint",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.8.1",
    "@emotion/styled": "11.8.1",
    "@fontsource/source-sans-pro": "4.5.3",
