    "lodash.kebabcase": "4.1.1",
    "lodash.orderby": "4.6.0",
    "lodash.startcase": "4.4.0",
    "match-sorter": "2.2.3",
    "normalize.css": "8.0.0",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
