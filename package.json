  },
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf .cache public",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,json,md,yml}\"",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "preserve": "yarn build",
    "serve": "gatsby serve",
    "test": "yarn lint"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
