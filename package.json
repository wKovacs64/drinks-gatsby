{
  "name": "drinks",
  "version": "0.1.0-dev.0",
  "description": "Static site to showcase your favorite cocktails",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
