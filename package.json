{
  "name": "drinks",
  "version": "0.1.0-dev.0",
  "description": "Gatsby site to highlight my high-functioning alcoholism",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/drinks.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/drinks/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "preserve": "yarn build",
    "serve": "gatsby serve",
    "test": "yarn lint"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "emotion": "9.2.6",
    "emotion-server": "9.2.6",
    "gatsby": "next",
    "gatsby-plugin-emotion": "2.0.0-beta.3",
    "gatsby-plugin-react-helmet": "next",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-emotion": "9.2.6",
    "react-helmet": "5.2.0",
    "tachyons": "4.11.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "prettier": "1.13.7",
    "rimraf": "2.6.2"
  }
}
