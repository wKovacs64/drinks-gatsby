{
  "name": "drinks",
  "version": "0.0.0-semantically-released",
  "description": "Static site and PWA to showcase your favorite cocktails",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/drinks.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/drinks/issues"
  },
  "scripts": {
    "build": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "develop": "cross-env GATSBY_HOT_LOADER=fast-refresh gatsby develop",
    "format": "prettier --write \"**/*.{js,ts,tsx,html,json,md,yml,yaml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "preserve": "yarn build",
    "serve": "gatsby serve",
    "test": "yarn lint",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@reach/dialog": "0.10.2",
    "@reach/skip-nav": "0.10.2",
    "@reach/visually-hidden": "0.10.2",
    "@wkovacs64/normalize.css": "8.0.1",
    "algoliasearch": "4.2.0",
    "dotenv": "8.2.0",
    "gatsby": "2.21.24",
    "gatsby-image": "2.4.3",
    "gatsby-plugin-algolia": "0.11.0",
    "gatsby-plugin-emotion": "4.3.1",
    "gatsby-plugin-manifest": "2.4.2",
    "gatsby-plugin-netlify": "2.3.2",
    "gatsby-plugin-offline": "3.2.1",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-sharp": "2.6.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-contentful": "2.3.3",
    "gatsby-source-filesystem": "2.3.1",
    "gatsby-transformer-remark": "2.8.7",
    "gatsby-transformer-sharp": "2.5.2",
    "git-current-commit": "1.1.0",
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-icons": "3.10.0",
    "react-instantsearch-dom": "6.4.0",
    "typeface-source-sans-pro": "0.0.75"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@wkovacs64/prettier-config": "3.0.0",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "6.8.0",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-wkovacs64": "5.0.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "semantic-release": "17.0.7"
  }
}
