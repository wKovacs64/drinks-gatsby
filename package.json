{
  "name": "drinks",
  "version": "0.0.0-semantically-released",
  "description": "Static site and PWA to showcase your favorite cocktails",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/drinks.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/drinks/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "cross-env NODE_ENV=test eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "preserve": "yarn build",
    "serve": "gatsby serve",
    "test": "yarn lint",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@fontsource/source-sans-pro": "4.5.1",
    "@reach/dialog": "0.16.2",
    "@reach/skip-nav": "0.16.0",
    "@reach/visually-hidden": "0.16.0",
    "@wkovacs64/normalize.css": "8.0.4",
    "@xstate/react": "1.6.2",
    "algoliasearch": "4.11.0",
    "dotenv": "10.0.0",
    "gatsby": "4.1.4",
    "gatsby-plugin-algolia": "0.23.0",
    "gatsby-plugin-emotion": "7.1.0",
    "gatsby-plugin-image": "2.1.2",
    "gatsby-plugin-manifest": "4.1.3",
    "gatsby-plugin-netlify": "3.14.0",
    "gatsby-plugin-offline": "5.1.3",
    "gatsby-plugin-react-helmet": "5.1.0",
    "gatsby-plugin-sharp": "4.1.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-contentful": "6.1.3",
    "gatsby-source-filesystem": "4.1.2",
    "gatsby-transformer-remark": "5.1.3",
    "gatsby-transformer-sharp": "4.1.0",
    "git-current-commit": "1.1.0",
    "lodash": "4.17.21",
    "prop-types": "15.7.2",
    "query-string": "7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-icons": "4.3.1",
    "xstate": "4.26.0"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@wkovacs64/prettier-config": "3.0.0",
    "babel-preset-gatsby": "2.1.2",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-plugin-wkovacs64": "13.7.1",
    "husky": "7.0.4",
    "lint-staged": "12.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0"
  }
}
