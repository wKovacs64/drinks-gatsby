{
  "name": "drinks",
  "version": "0.0.0-semantically-released",
  "description": "Static site and PWA to showcase your favorite cocktails",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/drinks.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/drinks/issues"
  },
  "scripts": {
    "build": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "develop": "cross-env GATSBY_HOT_LOADER=fast-refresh gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "preserve": "yarn build",
    "serve": "gatsby serve",
    "test": "yarn lint",
    "validate": "npm-run-all --silent --parallel lint build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@reach/dialog": "0.11.2",
    "@reach/skip-nav": "0.11.2",
    "@reach/visually-hidden": "0.11.1",
    "@wkovacs64/normalize.css": "8.0.1",
    "@xstate/react": "0.8.1",
    "algoliasearch": "4.4.0",
    "dotenv": "8.2.0",
    "gatsby": "2.24.55",
    "gatsby-image": "2.4.17",
    "gatsby-plugin-algolia": "0.11.2",
    "gatsby-plugin-emotion": "4.3.10",
    "gatsby-plugin-manifest": "2.4.28",
    "gatsby-plugin-netlify": "2.3.14",
    "gatsby-plugin-offline": "3.2.27",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.33",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-source-contentful": "2.3.43",
    "gatsby-source-filesystem": "2.3.28",
    "gatsby-transformer-remark": "2.8.34",
    "gatsby-transformer-sharp": "2.5.14",
    "git-current-commit": "1.1.0",
    "lodash": "4.17.20",
    "prop-types": "15.7.2",
    "query-string": "6.13.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-icons": "3.11.0",
    "typeface-source-sans-pro": "0.0.75",
    "xstate": "4.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@wkovacs64/prettier-config": "3.0.0",
    "commitizen": "4.2.1",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.8.1",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-wkovacs64": "8.0.0",
    "husky": "4.3.0",
    "lint-staged": "10.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "semantic-release": "17.1.1"
  }
}
