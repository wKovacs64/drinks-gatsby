    "eslint": "8.10.0",
    "eslint-plugin-wkovacs64": "13.12.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "semantic-release": "19.0.2"
