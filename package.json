    "@reach/skip-nav": "0.15.0",
    "@reach/visually-hidden": "0.15.0",
    "@wkovacs64/normalize.css": "8.0.1",
    "@xstate/react": "1.3.1",
    "algoliasearch": "4.8.6",
    "dotenv": "8.2.0",
    "gatsby": "3.2.1",
    "gatsby-plugin-algolia": "0.18.1",
    "gatsby-plugin-emotion": "6.2.0",
    "gatsby-plugin-image": "1.2.1",
